<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>8</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>RakDotNet.IO</Title>
    <Authors>Yui;Wincent01</Authors>
    <Description>Bit-level reads and writes library.</Description>
    <PackageProjectUrl>https://github.com/Wincent01/RakDotNet.IO</PackageProjectUrl>
